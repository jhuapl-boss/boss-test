      {"x_range":[200,712], "y_range":[200,712], "z_range":[1000,1050]},
      {"x_range":[0,512], "y_range":[1000,1512], "z_range":[10,12], "time_range":[2,4]}
    ],
    "cutout_read_throughput_cache_miss_test":[
